{
  "expo": {
    "name": "tests",
    "slug": "tests",
    "version": "1.0.0",
    
      "scheme": "tests",
      "web": {
        "bundler": "metro",
        "output": "static",
        "favicon": "./assets/favicon.png"
      },
      "plugins": [
      "expo-router"
      
      
    ],
      "experiments": {
        "typedRoutes": true
        
          ,"tsconfigPaths": true
        
      },
    
    "orientation": "portrait",

     "userInterfaceStyle": "light",
    
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "newArchEnabled": true
      }
    }
  }
}